{
  "name": "n01 Caller",
  "version": "0.1.2",
  "manifest_version": 3,
  "description": "n01のサイトで入力された点数を読み上げます。",
  "content_scripts": [{
    "matches": ["https://nakka.com/n01/*"],
    "js": ["js/jquery-3.6.0.min.js", "js/n01-caller.js"]
  }],
  "web_accessible_resources": [{
    "resources": [ "voice/*.mp3" ],
    "matches": [ "<all_urls>" ]
  }],
  "host_permissions": [
    "https://nakka.com/n01/*"
  ]
}